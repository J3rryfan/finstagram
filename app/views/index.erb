<% if current_user %>
  <div class="new-finstagram-post">
    <a href="/finstagram_posts/new" class="button">New Post</a>
  </div>
<% end %>

<html>
    <head>
        <link rel="stylesheet" type="text/css" href="../../../stylesheets/normalize.css">
        <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Roboto+Condensed:300italic,700italic,700,300">
        <link rel="stylesheet" type="text/css" href="../../../stylesheets/lib.css">
        <link rel="stylesheet" type="text/css" href="../../../stylesheets/app.css">
        <title>Finstagram</title>
    </head>

    <main role="main">
      <% @finstagram_posts.each do |finstagram_post| %>
      <%= erb :'shared/finstagram_post', locals: { finstagram_post: finstagram_post } %>
      <% end %>
  
    </main>



</html>